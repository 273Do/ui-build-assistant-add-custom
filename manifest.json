{
  "name": "UI Build Assistant",
  "version": "0.0.2",
  "manifest_version": 3,
  "description": "You can create layouts easily!",
  "permissions": ["activeTab", "storage", "scripting"],
  "host_permissions": ["http://*/*", "https://*/*", "file:///*"],
  "options_page": "index.html",
  "background": {
    "service_worker": "index.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/itu16.png",
      "32": "icons/itu32.png",
      "48": "icons/itu48.png",
      "128": "icons/itu128.png"
    }
  },
  "icons": {
    "16": "icons/itu16.png",
    "32": "icons/itu32.png",
    "48": "icons/itu48.png",
    "128": "icons/itu128.png"
  }
}
